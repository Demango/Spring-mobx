{
  "name": "mobx-react-test-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "6.11.3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "cross-env": "^5.0.5",
    "mobx": "^3.3.0",
    "mobx-react": "^4.3.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rest": "^2.0.0"
  },
  "scripts": {
    "watch": "webpack --watch -d --display-error-details",
    "build": "webpack",
    "test": "cross-env BABEL_ENV=test mocha -r babel-register src/main/js/test/helpers/browser.js src/main/js/test/**/*.spec.js",
    "cover": "cross-env BABEL_ENV=test nyc -x src/main/js/test mocha -r babel-register src/main/js/test/helpers/browser.js src/main/js/test/**/*.spec.js",
    "watch-test": "cross-env BABEL_ENV=test mocha -w -r babel-register src/main/js/test/helpers/browser.js src/main/js/test/**/*.spec.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.3.0",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0"
  }
}
